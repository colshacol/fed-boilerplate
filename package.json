{
  "author": {
    "name": "Barney Rubble",
    "email": "b@rubble.com",
    "url": "http://barnyrubble.tumblr.com/"
  },
  "description": "A boilerplate.",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.dev.js",
    "build:dev": "webpack --config ./config/webpack.dev.js",
    "webpack:watch": "webpack --config ./config/webpack.dev --watch",
    "remmy": "remmy",
    "test": "jest"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-router": "^3.0.5"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-flow-runtime": "^0.11.1",
    "babel-plugin-react-css-modules": "^2.1.3",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "flow-runtime": "^0.13.0",
    "jest": "^20.0.4",
    "jest-css-modules": "^1.1.0",
    "jest-enzyme": "^3.5.3",
    "jsx-control-statements": "^3.2.5",
    "prettier-eslint": "^6.4.2",
    "prettier-eslint-webpack-plugin": "^0.14.73",
    "prettier-webpack-plugin": "^0.2.1",
    "react-dom": "^15.6.1",
    "regenerator-runtime": "^0.10.5",
    "remmy": "^1.0.5",
    "strip-json-comments": "^2.0.1",
    "style-loader": "^0.18.2",
    "stylus-loader": "^3.0.1",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme"
    ],
    "testPathIgnorePatterns": [
      "./node_modules",
      "./source/.remmy"
    ],
    "testMatch": [
      "**/*.t.js"
    ],
    "verbose": true,
    "bail": false,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css)$": "<rootDir>/utils/test/cssMock.js"
    }
  }
}
